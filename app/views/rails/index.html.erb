<p id="notice"><%= notice %></p>

<h1>Rails</h1>

<table>
  <thead>
    <tr>
      <th>Generate</th>
      <th>Scaffold</th>
      <th>Studentprofile</th>
      <th>User</th>
      <th>First name</th>
      <th>Last name</th>
      <th>Date of birth</th>
      <th>Address</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Emergency contact name</th>
      <th>Emergency contact phone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rails.each do |rail| %>
      <tr>
        <td><%= rail.generate %></td>
        <td><%= rail.scaffold %></td>
        <td><%= rail.StudentProfile %></td>
        <td><%= rail.user_id %></td>
        <td><%= rail.first_name %></td>
        <td><%= rail.last_name %></td>
        <td><%= rail.date_of_birth %></td>
        <td><%= rail.address %></td>
        <td><%= rail.phone %></td>
        <td><%= rail.email %></td>
        <td><%= rail.emergency_contact_name %></td>
        <td><%= rail.emergency_contact_phone %></td>
        <td><%= link_to 'Show', rail %></td>
        <td><%= link_to 'Edit', edit_rail_path(rail) %></td>
        <td><%= link_to 'Destroy', rail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rail', new_rail_path %>
